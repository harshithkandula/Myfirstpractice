import java.util.ArrayList;

public class Batch {
       String timingType;
       int noOfParticipants;
       public Batch(){
    	   
       }
       
       public Batch(String timingType){
    	   this.timingType = timingType;
       }
       
       Batch bm = new Batch("Morning");
       Batch be = new Batch("Evening");
       Participants pt =new Participants();
       public void addParticipantsToBatch(Batch b, Participants p) {
    	  if( b == bm) {
    		  ArrayList<Batch> bmp = new ArrayList<Batch>();
    		  
    		  for(Batch bat: bmp) {
    			  p = new Participants(pt.getName(), pt.getDob(), "Morning", pt.getProfession(), pt.getPhno(), pt.getAddress()  );
    		  }
    	  }
    	   
    	   
       }
}
